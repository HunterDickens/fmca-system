# FMCA System Environment Variables
# Copy this file to .env and update the values for local development

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/fmca_db

# JWT Configuration
JWT_SECRET_KEY=your-super-secret-jwt-key-change-this-in-production

# Flask Configuration
FLASK_ENV=development
FLASK_APP=app.py

# Frontend API URL (for local development)
NEXT_PUBLIC_API_URL=http://localhost:5000

# Optional: Logging
LOG_LEVEL=INFO 